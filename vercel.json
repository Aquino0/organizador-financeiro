{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.0"
        },
        "*.php": {
            "runtime": "vercel-php@0.7.0"
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}